package constroctorOverloading;

public class Person {
	// 필드
	String name;
	int age;
	String job;

	// 1. 기본생성자
	public Person() {
		super();
	}

	// 2. 이름만 초기화하는 생성자
	public Person(String name) {
		super();
		this.name = name;
	}
	//3. 이름과 나이를 초기화하는 생성자
	public Person(String name, int age) {
		super();
		this.name = name;
		this.age = age;
	}
	//4. 나이와 이름을 초기화하는 생성자
	
	//5. 이름과 직업을 초기화하는 생성자
	
	//6. 직업과 이름을 초기화하는 생성자
	
	//7. 모든 필드를 초기화하는 생성자
}
