package Review;

public class Gugudan03 {
	public static void main(String[] args) {
//		9. 3단씩 출력하기 Gugudan03

//		i 초기값 1 설정, i는 9 이하, i는 3씩 증가
		for(int i = 1; i <= 9; i += 3) {
//		j 초기값 1 설정, j는 9 이하, j는 1씩 후열로 증가	
			for(int j = 1; j <= 9; j++) {
				for(int k = i; k < i + 3 && k <= 9; k++) {
					System.out.print(k + " * " + j + " = " + (k + j) + "\t");
				}
				System.out.println();
			}
			System.out.println();
		}
		
	}
}
