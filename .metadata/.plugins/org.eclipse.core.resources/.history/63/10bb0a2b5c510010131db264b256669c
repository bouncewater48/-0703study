package review;

public class Animal {
	//필드 //필드는 반드시 메인메소드 밖에서 만들어라
	String name; //동물 이름
	int age; //동물 나이
	String species; //동물의 종
	
	//메소드
	// 동물의 먹이를 먹는 메소드 매개변수 1개
	// => 동물이 00을 먹습니다
	void eat(String food) {
		System.out.println(name + "이/가 " + food + "를/을 먹습니다");
	}
	
	
	
	
	
	
	
	
	
	
	
	
}
