package task01;

public class Student {
	//서브 클래스 : Student
//  필드 : 학번, 전공
	String studentId;
	String major;
	
	//기본생성자
	public Student() {
		this("이름없음", 0, "값없음","전공없음");
	}

	public Student(String name, int age, String studentId, String major) {
		super(name, age);
		this.studentId = studentId;
		this.major = major;
		System.out.println("student 생성자 호출");
	}
	//부모 클래스의 introduce 메소드 오버라이딩
//		부모 메소드 호출
//		학번과 전공도 출력하는 메소드
	
	
}
