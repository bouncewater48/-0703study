package test_강승훈;

public class ForGugudan {
	public static void main(String[] args) {
		
		
	for(int i = 2; i <= 9; i += 3) {
		for(int j = 1; j <= 9; j++) {
			for(int k = i; k <= i+9 && k < 9; k++)
			System.out.print(j + " * " + k + " = " + (j * k) + "\t");
			}
		System.out.println();		
		}
	}
}
