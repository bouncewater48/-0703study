package task01;

public class Student {
	//서브 클래스 : Student
//  필드 : 학번, 전공
	String studentId;
	String major;
	
	//기본생성자
	public Student() {
		this("이름없음", 0, "값없음","전공없음");
	}

	public Student(String name, int age, String studentId, String major) {
		super();
		this.studentId = studentId;
		this.major = major;
	}
	
	
}
