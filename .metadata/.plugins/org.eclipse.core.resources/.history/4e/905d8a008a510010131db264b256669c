package constroctorThis03;
// 6번 : this()	자기자신의 생성자 호출
public class person {
	//필드
	String name;
	int age;
	String job;
	//생성자
	//기본생성자
	public person() {	
	}
	//매개변수 2개 생성자
	public person(String name, int age, String job) {
		this.name = name;
		this.age = age;
	}
	//매개변수 3개 생성자
	public person(String name, int age, String job) {
		super();
		this.name = name;
		this.age = age;
		this.job = job;
	}
	
	//메소드
	// printInfo() 정보 출력
	
	//greet() 000님 안녕하세요 출력
	
}
